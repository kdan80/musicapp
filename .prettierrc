{
    "trailingComma": "es5",
    "useTabs": false,
    "bracketSpacing": true,
    "printWidth": 100,
    "arrowParens": "avoid",
    "insertPragma": false,
    "requirePragma": false,
    "tabWidth": 4,
    "semi": false,
    "proseWrap": "always",
    "quoteProps": "as-needed",
    "singleQuote": true,
    "bracketSameLine": false,
    "jsxBracketSameLine": false,
    "jsxSingleQuote": true,
    "htmlWhitespaceSensitivity": "css",
    "singleAttributePerLine": true,
    "endOfLine": "lf",
    "overrides": [
        {
            "files": "*.json",
            "options": {
                "singleQuote": false
            }
        }
    ]
}